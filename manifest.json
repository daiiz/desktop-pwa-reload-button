{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "content_script.js"
    ],
    "run_at": "document_end"
  }],
  "description": "Reload button for Desktop PWA",
  "icons": {
      "128": "refresh.png",
      "48" : "refresh.png",
      "16" : "refresh.png"
  },
  "browser_action": {
      "default_icon": {
          "19": "refresh.png",
          "38": "refresh.png"
      },
      "default_title": "Reload"
  },
  "manifest_version": 2,
  "name": "Reload button for Desktop PWA",

  "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>"
  ],
  "version": "2019.4.1"
}
